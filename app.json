{
  "expo": {
    "name": "swarm",
    "slug": "swarm",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/appstore.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/appstore.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "versionCode": 10, 
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/appstore.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.kelvinkb.swarm"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/favicon.png"
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "2d9dc0c7-abe8-4aca-beaf-8c219b20b77e"
      }
    }
  }
}
